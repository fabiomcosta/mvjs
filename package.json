{
  "name": "@fabiomcosta/mvjs",
  "version": "1.7.3",
  "description": "Easily move JavaScript modules.",
  "repository": "https://github.com/fabiomcosta/mvjs",
  "main": "lib/index.js",
  "bin": {
    "mvjs": "lib/cli.js"
  },
  "scripts": {
    "test": "npm run lint && npm run flow && npm run unit",
    "flow": "flow",
    "flow-typed": "flow-typed",
    "lint": "eslint --fix src/",
    "unit": "jest",
    "prepublishOnly": "node -e 'require(`fs-extra`).removeSync(`lib/`)' && npm run build && npm t",
    "build": "babel src/ -d lib/ --copy-files",
    "build:watch": "npm run build -- --watch"
  },
  "keywords": [
    "mvjs",
    "move",
    "movejs",
    "movemodule"
  ],
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/(*.)(spec|test).js"
    ],
    "roots": [
      "<rootDir>/src/"
    ]
  },
  "author": "Fabio Miranda Costa",
  "license": "MIT",
  "dependencies": {
    "@babel/code-frame": "^7.5.5",
    "@babel/polyfill": "^7.4.4",
    "ast-types-flow": "0.0.7",
    "chalk": "^2.4.2",
    "debug": "^3.2.6",
    "fast-memoize": "^2.5.1",
    "find-up": "^2.1.0",
    "fs-extra": "^5.0.0",
    "jscodeshift": "^0.6.4",
    "yargs": "^11.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-flow": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^22.4.4",
    "eslint": "^4.19.1",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-jest": "^21.27.2",
    "flow-bin": "^0.66.0",
    "flow-typed": "^2.6.1",
    "jest": "^22.4.4"
  }
}
