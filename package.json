{
  "name": "@fabiomcosta/mvjs",
  "version": "1.15.0",
  "description": "Easily move files and directories around your JavaScript project.",
  "repository": "https://github.com/fabiomcosta/mvjs",
  "main": "lib/index.js",
  "bin": {
    "mvjs": "lib/cli.js"
  },
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  },
  "scripts": {
    "test": "npm run lint && npm run flow && npm run unit",
    "flow": "flow",
    "flow-typed": "flow-typed",
    "lint": "eslint --fix src/",
    "unit": "npm run build && jest",
    "prepublishOnly": "node -e 'require(`fs-extra`).removeSync(`lib/`)' && npm t",
    "build": "babel src/ -d lib/ --copy-files",
    "build:watch": "npm run build -- --watch"
  },
  "keywords": [
    "mvjs",
    "move",
    "movejs",
    "movemodule"
  ],
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/(*.)(spec|test).js"
    ],
    "roots": [
      "<rootDir>/src/"
    ]
  },
  "author": "Fabio Miranda Costa",
  "license": "MIT",
  "dependencies": {
    "@babel/code-frame": "^7.8.3",
    "ast-types-flow": "0.0.7",
    "chalk": "^3.0.0",
    "core-js": "^3.6.5",
    "debug": "^4.1.1",
    "enhanced-resolve": "^4.1.1",
    "find-up": "^4.1.0",
    "fs-extra": "^8.1.0",
    "ignore": "^5.1.4",
    "isbinaryfile": "^4.0.6",
    "jscodeshift": "^0.7.1",
    "yargs": "^15.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-flow": "^7.9.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "eslint": "^6.8.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-jest": "^23.9.0",
    "flow-bin": "^0.124.0",
    "flow-typed": "^3.1.0",
    "jest": "^26.0.1"
  }
}
