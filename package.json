{
  "name": "@fabiomcosta/mvjs",
  "version": "1.12.0",
  "description": "Easily move JavaScript modules.",
  "repository": "https://github.com/fabiomcosta/mvjs",
  "main": "lib/index.js",
  "bin": {
    "mvjs": "lib/cli.js"
  },
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  },
  "scripts": {
    "test": "npm run lint && npm run flow && npm run unit",
    "flow": "flow",
    "flow-typed": "flow-typed",
    "lint": "eslint --fix src/",
    "unit": "npm run build && jest",
    "prepublishOnly": "node -e 'require(`fs-extra`).removeSync(`lib/`)' && npm t",
    "build": "babel src/ -d lib/ --copy-files",
    "build:watch": "npm run build -- --watch"
  },
  "keywords": [
    "mvjs",
    "move",
    "movejs",
    "movemodule"
  ],
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/(*.)(spec|test).js"
    ],
    "roots": [
      "<rootDir>/src/"
    ]
  },
  "author": "Fabio Miranda Costa",
  "license": "MIT",
  "dependencies": {
    "@babel/code-frame": "^7.5.5",
    "ast-types-flow": "0.0.7",
    "chalk": "^2.4.2",
    "core-js": "^3.2.1",
    "debug": "^4.1.1",
    "find-up": "^4.1.0",
    "fs-extra": "^8.1.0",
    "isbinaryfile": "^4.0.4",
    "jscodeshift": "^0.6.4",
    "yargs": "^13.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/node": "^7.6.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-flow": "^7.0.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "eslint": "^6.4.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-flowtype": "^3.13.0",
    "eslint-plugin-jest": "^22.17.0",
    "flow-bin": "^0.108.0",
    "flow-typed": "^2.6.1",
    "jest": "^24.9.0"
  }
}
