{
  "name": "@fabiomcosta/mvjs",
  "version": "1.0.0",
  "description": "Moves JavaScript files and updates their reference on other modules inside the project.",
  "main": "lib/index.js",
  "scripts": {
    "test": "npm run flow && npm run unit",
    "flow": "flow",
    "flow-typed": "flow-typed",
    "unit": "jest",
    "prepublishOnly": "npm run build",
    "build": "babel src/ -d lib/",
    "build:watch": "nodemon --watch src/ --exec babel -- --watch src/ -d lib/"
  },
  "keywords": [
    "mvjs",
    "move",
    "movejs",
    "movemodule"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "ast-types-flow": "0.0.7",
    "@babel/polyfill": "^7.0.0-beta.39",
    "debug": "^3.1.0",
    "find-up": "^2.1.0",
    "glob": "^7.1.2",
    "jscodeshift": "^0.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.39",
    "@babel/core": "^7.0.0-beta.39",
    "@babel/node": "^7.0.0-beta.39",
    "@babel/preset-env": "^7.0.0-beta.39",
    "@babel/preset-flow": "^7.0.0-beta.39",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.39",
    "eslint": "^4.17.0",
    "flow-bin": "^0.64.0",
    "flow-typed": "^2.3.0",
    "jest": "^22.1.4",
    "nodemon": "^1.14.11"
  }
}
